<!DOCTYPE html>
<html>
<head>
  <title>OutlawsDemo</title>
  <%= stylesheet_link_tag 'home_good', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <img src="data:image/jpeg;base64, " />
      </div>

      <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav navbar-right">
          <li><li><%= link_to 'Dashboard', home_good_index_path %></li></li>
          <li><%= link_to 'Settings', home_good_setting_path %></li>
          <li><%= link_to 'Images', home_good_image_path %></li>
          <li><a href="#">Help</a></li>
        </ul>
        <form class="navbar-form navbar-right">
          <input type="text" class="form-control" placeholder="Search...">
        </form>
      </div>
    </div>
  </nav>

  <div class="container-fluid">
    <div class="row main">
      <%= yield %>
    </div>
  </div>

  <div class="panel panel-default">
    <div class="panel-body">
      Panel content
    </div>
    <div class="panel-footer">
      Panel footer
    </div>
  </div>


  <%= javascript_include_tag 'home_good', 'data-turbolinks-track' => true, defer: true %>
</body>
</html>
